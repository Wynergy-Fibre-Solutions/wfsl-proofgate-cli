{
  "$schema": "https://wfsl.dev/schemas/proofgate-manifest.v1.json",
  "wfsl": {
    "component": "wfsl-proofgate-cli",
    "role": "test-spine",
    "mode": "inhouse-only",
    "version": "0.1.0"
  },
  "spine": {
    "description": "WFSL in-house deterministic test spine. No third-party execution. No network access.",
    "execution_order": [
      "shell-guard"
    ]
  },
  "roles": {
    "shell-guard": {
      "repo_required": true,
      "evidence_required": false,
      "enforcement": "verify-only",
      "description": "Validates execution boundaries, shell integrity, and declared role compliance."
    }
  },
  "constraints": {
    "network_access": false,
    "telemetry": false,
    "third_party_execution": false,
    "mutation": false
  },
  "outputs": {
    "format": "json",
    "directory": "proofs",
    "verdict": "deterministic"
  }
}
